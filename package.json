{
    "name": "watch",
    "version": "1.18.0",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^3.0.3",
        "@material-ui/icons": "^3.0.1",
        "autoprefixer": "7.1.6",
        "babel-core": "6.26.0",
        "babel-jest": "20.0.3",
        "babel-loader": "^8.0.5",
        "babel-preset-react-app": "^7.0.2",
        "babel-runtime": "6.26.0",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "core-js": "^2.5.7",
        "css-loader": "^2.1.1",
        "csvtojson": "^2.0.8",
        "dotenv": "4.0.0",
        "dotenv-expand": "4.2.0",
        "express": "^4.16.3",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^3.0.1",
        "fs-extra": "3.0.1",
        "html-webpack-plugin": "^4.0.0-beta.5",
        "images-scraper": "^2.0.11",
        "jest": "^24.7.1",
        "lodash": "^4.17.19",
        "material-ui-chip-input": "^1.0.0-beta.7",
        "moment": "^2.22.2",
        "mongoose": "^5.7.5",
        "node-fetch": "^2.6.1",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.8",
        "promise": "8.0.1",
        "prop-types": "^15.6.2",
        "raf": "3.4.0",
        "react": "^16.5.1",
        "react-autosuggest": "^9.4.1",
        "react-dev-utils": "^8.0.0",
        "react-dom": "^16.5.1",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0",
        "resolve": "1.6.0",
        "shrink-ray-current": "^3.0.2",
        "style-loader": "0.19.0",
        "sw-precache-webpack-plugin": "^0.11.5",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "url-loader": "0.6.2",
        "webpack": "^4.29.6",
        "webpack-dev-server": "^3.2.1",
        "webpack-manifest-plugin": "^2.0.4",
        "whatwg-fetch": "2.0.3",
        "why-did-you-update": "^0.1.1"
    },
    "devDependencies": {
        "babel-eslint": "^7.2.3",
        "enzyme": "^3.6.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-loader": "1.9.0",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-jsx-a11y": "^6.1.1",
        "eslint-plugin-react": "^7.10.0",
        "source-map-explorer": "^1.6.0"
    },
    "scripts": {
        "start": "node scripts/start-dev.js",
        "start-prod": "node scripts/start-prod.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js --env=jsdom",
        "server": "node ./src/server/server.js",
        "lint": "./node_modules/eslint/bin/eslint.js ./src",
        "lint-fix": "./node_modules/eslint/bin/eslint.js ./src --fix",
        "start-all": "node scripts/start-all.js",
        "start-all-prod": "node scripts/start-all.js --prod",
        "loc": "echo \"JavaScript:\" && wc -l src/**/*.js && echo \"\nCSS:\" && wc -l src/**/*.css",
        "analyze": "source-map-explorer build/static/js/main.*"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,mjs}"
        ],
        "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setup.js",
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node",
            "mjs"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
